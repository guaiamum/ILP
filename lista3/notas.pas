program notas;var	i,j,k,l,v,t: integer;begin	read(v);	t:=0; i:=0; j:=0; k:=0; l:=0;	while v<>0 do	begin		t:=t+1;		writeln('Teste ',t);		if v>=50 then		begin			i:=v div 50;			j:=(v-(50*i)) div 10;			k:=(v-(50*i+10*j)) div 5;			l:=(v-(50*i+10*j+5*k)) div 1		end		else		if (v<50) and (v>=10) then		begin			i:=0;			j:=v div 10;			k:=(v-(10*j)) div 5;			l:=(v-(10*j+5*k)) div 1		end		else		if (v<10) and (v>=5) then		begin			i:=0;			j:=0;			k:=v div 5;			l:=(v-(5*k)) div 1		end		else		if (v<5) and (v>=1) then		begin			i:=0;			j:=0;			k:=0;			l:=v div 1		end;		writeln(i,' ',j,' ',k,' ',l);		writeln( );		read(v)	end;end.