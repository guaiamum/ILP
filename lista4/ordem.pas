program bla;var 	x,y,imenor,troca: integer;	num: array [1..10] of integer;begin	for x:=1 to 10 do	read(num[x]);	for x:=1 to 10 do		Begin			imenor:=x;			for y:=x+1 to 10 do 			Begin				if num[y]<num[imenor] then 					imenor:=y; // Se ver que o valor contido no ’ndice y Ž menor do que o valor do ’ndice definido com o imenor, muda											End;			if num[x]<>num[imenor] then // Se realmente forem diferentes muda			Begin				troca:=num[x]; // Tem-se de declarar uma vari‡vel para a troca, n‹o perdendo-se assim valores											num[x]:=num[imenor];				num[imenor]:=troca;			End;		End;	for x:=1 to 10 do	writeln(num[x]);end.